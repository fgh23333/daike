<view class="flex-col page">
	<view class="groundContainer">
		<image class="shrink-0 image" src="https://onlineimg.alni.eu.org/raw/daike/my/my_background.png" />
		<view class="userInfo">
			<image class="userIcon" src="../../image/creative.png" />
			<text class="userName">
				靳取
				<!-- {{ username }} -->
			</text>
			<text class="personal">个人</text>
			<view class="userButton" bindtap="goto">账户信息
				<van-icon name="arrow" />
			</view>
		</view>
		<view class="list">
			<image src="https://onlineimg.alni.eu.org/raw/daike/my/my_recbg1.png" class="image1" />
			<image src="https://onlineimg.alni.eu.org/raw/daike/my/my_recbg2.png" class="image2" />
			<text class="userList">代课订单</text>
			<view class="allUserListHover" bindtap="loguUserList">
				<text class="allUserList">全部订单</text>
				<van-icon name="arrow" class="arrow" />
			</view>
			<view class="orderFilterHover">
				<view class="orderFilter">
					<text>我发布的</text>
					<image src="https://onlineimg.alni.eu.org/raw/daike/my/my_post.png" />
				</view>
				<view class="orderFilter">
					<text>我接单的</text>
					<image src="https://onlineimg.alni.eu.org/raw/daike/my/my_order.png" />
				</view>
				<view class="orderFilter">
					<text>待完成的</text>
					<image src="https://onlineimg.alni.eu.org/raw/daike/my/my_uncompleted.png" />
				</view>
				<view class="orderFilter">
					<text>已完成的</text>
					<image src="https://onlineimg.alni.eu.org/raw/daike/my/my_completed.png" />
				</view>
			</view>

		</view>
	</view>
</view>